LEVEL 4:
for student in range(0,19):
    while not(plane.isAboveStudent()):
        plane.move()
    if not (plane.isAboveSally()):
        plane.dropWater()
while(plane.check() is not 'TARGET'):
    plane.move()
plane.move()

LEVEL 5:
#Let's find Sally!
max = 20
min = 0
toReturn = None
while(max >= min):
    mid = (min+max)/2
    if(plane.askName(mid)<"Sally"):
        min = mid+1
    elif(plane.askName(mid)>"Sally"):
        max = mid-1
    else:
        toReturn = mid
        break

LEVEL 6:
def loopy(plane, room):
    plane.markRoom()
    if plane.isRoomGoal():
        return False
    for n in plane.roomNeighbors(room):
        if not plane.isMarked(n):
            plane.goToRoom(n)
            b = loopy(plane, n)
            if b:
                plane.goToRoom(room)
            return True


a = plane.whereAmI()
loopy(plane,a)
